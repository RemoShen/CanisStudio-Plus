{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        // "module": "es6",
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
        "allowJs": true,
        "downlevelIteration": true,
        "allowSyntheticDefaultImports": true,
        "resolveJsonModule": true,
        "esModuleInterop": true
    }
}